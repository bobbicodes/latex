(defn digits [n]
  (if (> n 10)
    (conj (digits (quot n 10))
          (rem n 10))
    [n]))

(defn render-num [n]
  (let [digits (digits n)
        count  (count digits)]
    (into [:svg {:width    (* 9 count)
                 :view-box (str "0 0 " (* 500 count) " " 1000)}]
          (for [d (range count)]
            [:path {:transform (str "translate(" (* d 500) ",0)")
                    :stroke    "#000000"
                    :d         ((keyword (str (get digits d))) numbers)}]))))

